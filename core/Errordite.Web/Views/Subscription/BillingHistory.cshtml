
@using Errordite.Web.Models.Account
@using Errordite.Web.Models.Subscription
@model Errordite.Web.Models.Subscription.SubscriptionViewModel
           
@{
    ViewBag.Title = "Billing History";
}

@section header
{
    @Html.Partial("Account/Tabs") 
}

<section class="centered">
	<div class="content">
		<div class="sidenav">
			@Html.Partial("Subscription/SubscriptionTabs", SubscriptionTab.BillingHistory)
			<div class="sidenav-content billing">
				<div class="sidenav-tab active" id="billinghistory">
					You do not currently have any billing history.
				</div>
			</div>
		</div>
	</div>
</section>


