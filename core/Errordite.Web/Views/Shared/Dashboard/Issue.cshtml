@using Errordite.Web.Extensions
@model Errordite.Web.Models.Issues.IssueItemViewModel

<tr>
	<td>
		<a class="word-break" href="@Url.Issue(Model.IssueId)">@Model.Name</a><br />
		<span class="table-info">@Model.Status</span>
	</td>
	<td style="text-align:right;width:100px">@Model.LastError</td>
	<td style="text-align:right;width:50px"><span class="badge badge-info">@Model.ErrorCount</span></td>
</tr>
        