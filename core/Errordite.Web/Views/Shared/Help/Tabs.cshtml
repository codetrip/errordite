@using Errordite.Web.Extensions
@using Errordite.Web.Models.Help
@model HelpTab
         
<div class="tabs-container">
    <ul class="tabs">
        <li @(Model == HelpTab.GettingStarted ? " class=active" : " class=inactive") >
            <a href="@Url.GettingStarted()">Getting Started</a>
        </li>
        <li @(Model == HelpTab.Pricing ? " class=active" : " class=inactive") >
            <a href="@Url.Pricing()">Pricing</a>
        </li>
        <li @(Model == HelpTab.Features ? " class=active" : " class=inactive") >
            <a href="@Url.Features()">Features</a>
        </li>
        <li @(Model == HelpTab.Client ? " class=active" : " class=inactive") >
            <a href="@Url.Client()">.NET Client</a>
        </li>
        <li @(Model == HelpTab.SendErrorWithJson ? " class=active" : " class=inactive") >
            <a href="@Url.JsonFormat()">Send error with JSON</a>
        </li>
    </ul>
</div>